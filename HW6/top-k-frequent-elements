class Solution(object):
    def topKFrequent(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: List[int]
        """
        
        heap = []
        counter = {}

        for i in nums:
            counter[i] = counter.get(i,0) + 1

        for key, val in counter.items():
            heapq.heappush(heap, (-val, key))

        res = []
        while len(res) < k:
            res.append(heapq.heappop(heap)[1])
        return res
